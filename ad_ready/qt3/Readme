    vi .bashrc

    add comtent : export PATH=$PATH:/home/jiesse/.../bin  only use by jiesse


    source /etc/profile

    system(cmdline);  cmdline is the command line ;

    etc: sprintf(cmdline,"rm -rf %s\n",filename);

    effect: remove();

